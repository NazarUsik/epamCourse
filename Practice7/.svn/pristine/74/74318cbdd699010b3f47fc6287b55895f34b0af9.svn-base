package ua.nure.usik.practice7.entity;

public class TTC {
    private String longRange;
    private String sightingRange;
    private String clipAvailability;
    private String opticsAvailability;

    public TTC(){

    }

    public TTC(TTC ttc) {
        this.longRange = ttc.longRange;
        this.sightingRange = ttc.sightingRange;
        this.clipAvailability = ttc.clipAvailability;
        this.opticsAvailability = ttc.opticsAvailability;
    }

    public String getLongRange() {
        return longRange;
    }

    public void setLongRange(String longRange) {
        this.longRange = longRange;
    }

    public String getSightingRange() {
        return sightingRange;
    }

    public void setSightingRange(String sightingRange) {
        this.sightingRange = sightingRange;
    }

    public String getClipAvailability() {
        return clipAvailability;
    }

    public void setClipAvailability(String clipAvailability) {
        this.clipAvailability = clipAvailability;
    }

    public String getOpticsAvailability() {
        return opticsAvailability;
    }

    public void setOpticsAvailability(String opticsAvailability) {
        this.opticsAvailability = opticsAvailability;
    }

    @Override
    public String toString() {
        return  "   LongRange: " + longRange + "\n" +
                "   SightingRange: " + sightingRange + "\n" +
                "   ClipAvailability: " + clipAvailability + "\n" +
                "   OpticsAvailability: " + opticsAvailability + "\n";
    }
}
