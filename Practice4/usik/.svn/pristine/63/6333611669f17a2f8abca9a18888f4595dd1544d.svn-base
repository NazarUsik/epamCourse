package ua.nure.usik.practice1;

public class Part5{
	public static void main(String[] args){
		try {
			int n = Integer.parseInt(args[0]);
			int res = 0;
			while (n != 0) {
				res += (n % 10);
				n /= 10;
			}
			System.out.println(res);
		} catch (IndexOutOfBoundsException e){
			System.out.println("Error");
		}
	}
}